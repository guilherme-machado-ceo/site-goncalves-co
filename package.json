{
  "name": "goncalves-co-website",
  "version": "1.0.0",
  "description": "Gonçalves & Co. - Comércio, Gestão e Logística",
  "main": "index.html",
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "sass src/scss:dist/css --style compressed",
    "build:js": "webpack --mode production",
    "dev": "npm run build && http-server -p 8080 -o",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "business",
    "logistics",
    "commerce"
  ],
  "author": "Gonçalves & Co.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "babel-loader": "^10.0.0",
    "bootstrap": "^5.3.2",
    "gh-pages": "^6.1.0",
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8"
  }
}
